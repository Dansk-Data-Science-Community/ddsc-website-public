[run]
source = ddsc_web
omit =
    */migrations/*
    */tests.py
    */test_*.py
    */__init__.py
    */manage.py
    */wsgi.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod
